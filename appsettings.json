{
  "ConnectionStrings": {
    "Redis": "%REDIS_URL%"
  },
  "ApiConfig": {
    "CorsOrigin": "*",
    "Swagger": true,
    "CacheConfig": {
      //6 da manh√£ de domingo.
      "ExpireEvery": "0 6 * * 0"
    },
    "Authorization": {
      "Activate": true,
      "ApiHeader": "ApiKey",
      "ApiKey": "%API_KEY%"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
